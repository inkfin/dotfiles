<?xml version="1.0" encoding="utf-8" ?>
<!-- BuildConfiguration.Engine.xml -->
<!-- This file is used to configure the **Engine Specific** build system for Unreal Engine projects. It specifies settings for FASTBuild, parallel execution, and project file generation. -->
<!-- Located in $EngineRoot\Engine\Saved\UnrealBuildTool\BuildConfiguration.xml -->
<!-- Document: https://dev.epicgames.com/documentation/en-us/unreal-engine/build-configuration-for-unreal-engine -->
<Configuration xmlns="https://www.unrealengine.com/BuildConfiguration">
	<BuildConfiguration>
        <bAllowFASTBuild>true</bAllowFASTBuild>
        <bUseUBTMakefiles>true</bUseUBTMakefiles>
		<!-- <DefaultWarningLevel>Off</DefaultWarningLevel> -->
        <!-- [Experimental]: Disable UE 5.5+ Unreal Build Accelerator to use C++ Dynamic Debugging -->
        <!--   reference: https://learn.microsoft.com/en-us/visualstudio/debugger/cpp-dynamic-debugging?view=vs-2022#enable-c-dynamic-debugging-in-unreal-engine -->
		<bAllowUBAExecutor>false</bAllowUBAExecutor>
		<bAllowUBALocalExecutor>false</bAllowUBALocalExecutor>
	</BuildConfiguration>
    <FASTBuild>
        <FBuildExecutablePath>Z:\FBuild\FBuild.exe</FBuildExecutablePath>
        <bStopOnError>true</bStopOnError>
        <bEnableCaching>true</bEnableCaching>
        <CacheMode>ReadWrite</CacheMode>
        <bEnableDistribution>false</bEnableDistribution>
        <FBuildCachePath>${EngineRoot}\.fbcache</FBuildCachePath>
        <!-- <FBuildBrokeragePath>\\INK_DESKTOP\Brokerage</FBuildBrokeragePath> -->
    </FASTBuild>
    <ParallelExecutor>
        <!-- <MaxProcessorCount>6</MaxProcessorCount> -->
        <!-- <ProcessorCountMultiplier>0.8</ProcessorCountMultiplier> -->
        <!-- <ProcessPriority>BelowNormal</ProcessPriority> -->
    </ParallelExecutor>
    <WindowsPlatform>
        <Compiler>VisualStudio2022</Compiler>
        <CompilerVersion>14.38.33130</CompilerVersion>
		<WindowsSdkVersion>10.0.22621.0</WindowsSdkVersion>
        <!-- <Compiler>Clang</Compiler> -->
        <!-- <bAllowClangLinker>false</bAllowClangLinker> -->
        <!-- <CompilerVersion>16.0.4</CompilerVersion> -->
        <!-- <Compiler>VisualStudio2022</Compiler> -->
		<!-- <WindowsSdkVersion>10.0.22621.0</WindowsSdkVersion> -->
        <!-- [Experimental]: Disable UE 5.5+ Unreal Build Accelerator to use C++ Dynamic Debugging -->
        <!-- <bDynamicDebugging>true</bDynamicDebugging> -->
    </WindowsPlatform>
    <ProjectFileGenerator>
        <!-- <Format>CMake</Format> -->
        <!-- <Format>VisualStudioCode</Format> -->
        <!-- <bGenerateIntelliSenseData>true</bGenerateIntelliSenseData> -->
        <!-- <bIncludeEngineSource>true</bIncludeEngineSource> -->
        <!-- <bIncludeShaderSource>true</bIncludeShaderSource> -->
        <!-- <bIncludeConfigFiles>true</bIncludeConfigFiles> -->
        <!-- <bIncludeEnginePrograms>true</bIncludeEnginePrograms> -->
    </ProjectFileGenerator>
</Configuration>
